{
  "name": "randbox",
  "version": "1.1.0",
  "description": "RandBox Core - Utility library to generate anything random with TypeScript support",
  "homepage": "https://randbox.top",
  "author": "027xiguapi <458813868@qq.com>",
  "bugs": {
    "url": "https://github.com/027xiguapi/randbox/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/027xiguapi/randbox.git",
    "directory": "packages/randbox"
  },
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/randbox.d.ts",
  "module": "dist/randbox.js",
  "main": "dist/randbox.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/randbox.d.ts",
      "import": "./dist/randbox.js",
      "require": "./dist/randbox.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite examples --port 3300 --host 0.0.0.0",
    "test:coverage": "vitest run --coverage",
    "build": "vite build",
    "build:dev": "vite build --watch",
    "test": "pnpm run build && pnpm run test:legacy",
    "test:legacy": "ava __tests__/test.**.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "build:minified": "vite build --mode production",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "randbox",
    "random",
    "generator",
    "test",
    "mersenne",
    "typescript",
    "utility",
    "core"
  ],
  "github": "https://github.com/027xiguapi/randbox",
  "maintainers": [
    {
      "name": "027xiguapi",
      "email": "458813868@qq.com",
      "web": "https://randbox.top/"
    }
  ]
}
