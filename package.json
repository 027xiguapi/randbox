{
  "name": "randbox",
  "main": "./dist/index.cjs",
  "module": "./dist/index.esm.mjs",
  "types": "./dist/index.d.ts",
  "version": "1.0.0",
  "description": "RandBox - Utility library to generate anything random with TypeScript support",
  "homepage": "https://randbox.top",
  "author": "027xiguapi <458813868@qq.com>",
  "bugs": {
    "url": "https://github.com/027xiguapi/randbox/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/027xiguapi/randbox.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ava": "^6.4.1",
    "coveralls": "^3.1.1",
    "docpress": "0.7.1",
    "eslint": "^8.57.0",
    "git-update-ghpages": "1.3.0",
    "lodash": "^4.17.21",
    "nyc": "^15.1.0",
    "rollup": "^4.52.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "npm run build:ts && npm run build:rollup",
    "build:ts": "tsc",
    "build:rollup": "rollup -c",
    "build:legacy": "node scripts/build-legacy.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov --report-dir=./coverage > ./coverage/lcov.info",
    "docs": "docpress build",
    "docs:publish": "git-update-ghpages -e",
    "lint": "eslint src/**/*.ts",
    "test": "npm run build && npm run test:legacy",
    "test:legacy": "ava test/test.**.js",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "randbox",
    "random",
    "generator",
    "test",
    "mersenne",
    "typescript",
    "utility"
  ],
  "github": "https://github.com/027xiguapi/randbox",
  "maintainers": [
    {
      "name": "027xiguapi",
      "email": "458813868@qq.com",
      "web": "https://randbox.top/"
    }
  ]
}
